       function doTest() {
         var c = document.createElement("span");
         c.style.display = "table-cell";
         c.textContent = "Row 22, Col 1";
         var t = document.getElementById("t");
         t.parentNode.insertBefore(c, t);
         document.documentElement.className = "";
